{
  "name": "codeurnomade",
  "version": "0.1.0",
  "description": "A shopping list application built with Vue.js, JSON server, and Axios.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run start-proxy\" \"vite\"",
    "start-proxy": "node proxy.js",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint --fix --ext .js,.ts,.vue src"
  },
  "keywords": [
    "Vue.js",
    "JSON server",
    "Axios",
    "shopping list",
    "MEVN stack"
  ],
  "author": "matdomichard",
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue-leaflet/vue-leaflet": "^0.9.0",
    "axios": "^1.4.0",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "d3": "^7.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "leaflet": "^1.9.3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "axios-mock-adapter": "^1.21.4",
    "concurrently": "^8.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
